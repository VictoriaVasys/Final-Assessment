<%= form_for(:session, url: root_path) do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
  <%= f.submit "Log In"%>
<% end %>

<%= link_to "Sign up by clicking here.", sign_up_path %>
